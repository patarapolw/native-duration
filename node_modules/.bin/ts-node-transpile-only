#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/patarapolw/projects/aloud/packages/native-duration/node_modules/.pnpm/ts-node@9.1.0_typescript@4.1.2/node_modules/ts-node/dist/bin-transpile.js/node_modules:/home/patarapolw/projects/aloud/packages/native-duration/node_modules/.pnpm/ts-node@9.1.0_typescript@4.1.2/node_modules/ts-node/dist/node_modules:/home/patarapolw/projects/aloud/packages/native-duration/node_modules/.pnpm/ts-node@9.1.0_typescript@4.1.2/node_modules/ts-node/node_modules:/home/patarapolw/projects/aloud/packages/native-duration/node_modules/.pnpm/ts-node@9.1.0_typescript@4.1.2/node_modules:/home/patarapolw/projects/aloud/packages/native-duration/node_modules/.pnpm/node_modules:/home/patarapolw/projects/aloud/packages/native-duration/node_modules:/home/patarapolw/projects/aloud/packages/node_modules:/home/patarapolw/projects/aloud/node_modules:/home/patarapolw/projects/node_modules:/home/patarapolw/node_modules:/home/node_modules:/node_modules:/home/patarapolw/projects/aloud/packages/native-duration/node_modules/ts-node/dist/bin-transpile.js/node_modules:/home/patarapolw/projects/aloud/packages/native-duration/node_modules/ts-node/dist/node_modules:/home/patarapolw/projects/aloud/packages/native-duration/node_modules/ts-node/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
else 
  exec node  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
fi
