#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/patarapolw/projects/aloud/packages/understandable-duration/node_modules/.pnpm/typescript@4.1.2/node_modules/typescript/bin/tsc/node_modules:/home/patarapolw/projects/aloud/packages/understandable-duration/node_modules/.pnpm/typescript@4.1.2/node_modules/typescript/bin/node_modules:/home/patarapolw/projects/aloud/packages/understandable-duration/node_modules/.pnpm/typescript@4.1.2/node_modules/typescript/node_modules:/home/patarapolw/projects/aloud/packages/understandable-duration/node_modules/.pnpm/typescript@4.1.2/node_modules:/home/patarapolw/projects/aloud/packages/understandable-duration/node_modules/.pnpm/node_modules:/home/patarapolw/projects/aloud/packages/understandable-duration/node_modules:/home/patarapolw/projects/aloud/packages/node_modules:/home/patarapolw/projects/aloud/node_modules:/home/patarapolw/projects/node_modules:/home/patarapolw/node_modules:/home/node_modules:/node_modules:/home/patarapolw/projects/aloud/packages/understandable-duration/node_modules/.pnpm/ts-node@9.1.0_typescript@4.1.2/node_modules/ts-node/node_modules:/home/patarapolw/projects/aloud/packages/understandable-duration/node_modules/.pnpm/ts-node@9.1.0_typescript@4.1.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.1.2/node_modules/typescript/bin/tsc" "$@"
else 
  exec node  "$basedir/../../../../../typescript@4.1.2/node_modules/typescript/bin/tsc" "$@"
fi
